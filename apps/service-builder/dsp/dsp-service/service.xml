<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_2_0.dtd">

<service-builder dependency-injector="ds" package-path="com.liferay.blade.samples.dspservicebuilder">
	<namespace>DSPCOUNTRY</namespace>
	<entity
		local-service="true"
		name="Country"
		remote-service="false"
		table="country"
		uuid="false"
	>
		<column db-name="id" name="countryId" primary="true" type="long" />
		<column db-name="name" name="countryName" type="String" />
	</entity>
</service-builder>